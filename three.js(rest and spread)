// elements are  combined
function adNUmbers(a,b,c,...other){
   
    return a+b+c;
    
}
const res=adNUmbers(2,5,6,8,9)
    console.log(res)
    //spread
    // elements combines are scattered
    
    var names=["ajay","anuj","vivek"];
    
    function getNames(name1,name2,name3){
        console.log(name1,name2,name3);
    }
    getNames(names[0],names[1],names[2]);
    getNames(...names);
    //using spread
    
